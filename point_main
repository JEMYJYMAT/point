using namespace std;
#include <iostream>
#include "point.h"
#include "lines.h"

int main()
{
	point p1(7, 12), p2(3, 9);
	lines l1(p1, p2),l2;
	l2 = l1 + 30;
	cout << l1 << endl;
	cout << l2 << endl;
	    
}
